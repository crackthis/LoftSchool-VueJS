.header {
    display: flex;
    margin-bottom: 60px;
    justify-content: flex-start;
}

.page-content {
    padding: 60px 0 20px;
    background: url("/src/images/bg/admin.jpg") center center / cover no-repeat;
    flex: 1;
}

.title {
    margin-right: 60px;
    font-size: 21px;
    font-weight: bold;
    color: #414c63;
}

.skills {
    display: flex;
    flex-wrap: wrap;
}

.item {
    width: calc(100% / 2 - 30px);
    margin-right: 30px;
}
